public class Animal {
    private String nome;
    private String raca;
    private int idade;

    public Animal(String nome, String raca, int idade) {
        this.nome = nome;
        this.raca = raca;
        this.idade = idade;
    }

    public void latir() {
        System.out.println("Au au!");
    }

    public static void main(String[] args) {
        Animal cachorro1 = new Animal("Rex", "Labrador", 3);
        cachorro1.latir(); // Sa√≠da: Au au!
    }
}
